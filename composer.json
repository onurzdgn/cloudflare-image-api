{
    "name": "onurozdogan/cloudflare-image-api",
    "description": "Cloudflare Image API PHP Library",
    "keywords": [
        "cloudflare",
        "image",
        "api",
        "php",
        "library",
        "laravel"
    ],
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "onurozdogan\\CloudflareImageApi\\": "src/"
        }
    },
    "authors": [
        {
            "role": "Developer",
            "name": "Onur ÖZDOĞAN",
            "email": "onur_oz_dogan@hotmail.com",
            "homepage": "https://onurozdogan.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "onurozdogan\\CloudflareImageAPI\\CloudflareImageAPIServiceProvider"
            ],
            "aliases": {
                "CloudflareImageAPI": "onurozdogan\\CloudflareImageAPI\\Facade"
            }
        }
    }
}
